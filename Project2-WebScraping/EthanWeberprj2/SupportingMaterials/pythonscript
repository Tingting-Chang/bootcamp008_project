    "def transnut(df2):\n",
    "    df2.columns = ['name', 'stars', 'rev_count', 'nutrients', 'pics', 'time', 'ingredients']\n",
    "    df3 = pd.DataFrame()\n",
    "    for i in df2.loc[:, 'nutrients']:\n",
    "        if i == None:\n",
    "            cals = None\n",
    "            fat = None\n",
    "            carbs = None\n",
    "            protein = None\n",
    "            cholesterol = None\n",
    "            df3 = df3.append(pd.Series([cals, fat, carbs, protein, cholesterol, sodium]), ignore_index = True)\n",
    "            df3.columns = ['cals', 'fat_cal_pct', 'carb_cal_pct', 'protein_cal_pct', 'cholesterol_mg', 'sodium_mg']\n",
    "            df2 = df2.loc[:,['name', 'stars', 'rev_count', 'pics', 'time', 'ingredients']]\n",
    "            return pd.concat([df2, df3], axis = 1)\n",
    "        else:\n",
    "            pass\n",
    "        j = i.split(\"u'\")\n",
    "        try:\n",
    "            cals = float(j[1].split(\" \")[0])\n",
    "        except:\n",
    "            cals = None\n",
    "        try:\n",
    "            fat = float(j[2].split(\" \")[0])*900.0/float(j[1].split(\" \")[0])\n",
    "        except:\n",
    "            fat = None\n",
    "        try:\n",
    "            carbs = float(j[3].split(\"g\")[0])*400.0/float(j[1].split(\" \")[0])\n",
    "        except:\n",
    "            carbs = None\n",
    "        try:\n",
    "            protein = float(j[4].split(\" \")[0])\n",
    "        except:\n",
    "            protein = None\n",
    "        try:\n",
    "            cholesterol = float(j[5].split(\" \")[0].replace('<', ''))\n",
    "        except:\n",
    "            cholesterol = None\n",
    "        try: \n",
    "            sodium = float(j[6].split(\" \")[0])\n",
    "        except:\n",
    "            sodium = None\n",
    "        df3 = df3.append(pd.Series([cals, fat, carbs, protein, cholesterol, sodium]), ignore_index = True)\n",
    "    df3.columns = ['cals', 'fat_cal_pct', 'carb_cal_pct', 'protein_cal_pct', 'cholesterol_mg', 'sodium_mg']\n",
    "    df2 = df2.loc[:,['name', 'stars', 'rev_count', 'pics', 'time', 'ingredients']]\n",
    "    return pd.concat([df2, df3], axis = 1)"
