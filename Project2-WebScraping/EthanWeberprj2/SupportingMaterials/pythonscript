   "source": [
    "def evens(x):\n",
    "    out = []\n",
    "    for i in range(len(x)):\n",
    "        if i % 2 != 0:\n",
    "            out.append(x[i])\n",
    "        else:\n",
    "            continue\n",
    "    return out\n",
    "\n",
    "def ingred(df):\n",
    "    df2 = pd.DataFrame()\n",
    "    for j in df.loc[:,'ingredients']:\n",
    "        jlist = []\n",
    "        j = evens(j.split(\"'\"))\n",
    "        for i in j:\n",
    "            jlist.append(' '.join(filter(str.isalnum, i.replace(\"'\", \"\").split(' '))))\n",
    "        jlist = pd.Series({'ing' : jlist})\n",
    "        #print jlist\n",
    "        #print type(jlist)\n",
    "        df2 = df2.append(pd.Series(jlist), ignore_index = True)\n",
    "    return pd.concat([df, df2], axis = 1).loc[:, ['name', 'stars', 'rev_count', 'pics', 'time', 'cals', 'fat_cal_pct', 'carb_cal_pct', 'protein_cal_pct', 'cholesterol_mg', 'sodium_mg', 'ing']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def fix(j):\n",
    "    if j != None:\n",
    "        j = j.replace(' ', '').replace('m', '').split('h')\n",
    "        if 'd' in j[0]:\n",
    "            j = 24*60\n",
    "        elif (len(j) == 2):\n",
    "            if j[1] == '':\n",
    "                j = int(j[0])*60\n",
    "            else:\n",
    "                j = int(j[0])*60 + int(j[1])\n",
    "        else:\n",
    "            j = int(j[0])\n",
    "        return j\n",
    "    else:\n",
    "        return None\n",
    "        \n",
    "        \n",
    "def outputtimedf(x):\n",
    "    dfout = pd.DataFrame()\n",
    "    for i in range(x.loc[: , 'time' ].count()):\n",
    "        try:\n",
    "            cooktime = fix(ast.literal_eval(x.loc[i, 'time' ])['Cook'])\n",
    "        except:\n",
    "            cooktime = None\n",
    "        try:\n",
    "            totaltime = fix(ast.literal_eval(x.loc[i, 'time' ])['Ready In'])\n",
    "        except:\n",
    "            totaltime = None\n",
    "        try:\n",
    "            preptime = fix(ast.literal_eval(x.loc[i, 'time' ])['Prep'])\n",
    "        except:\n",
    "            preptime = None\n",
    "        if cooktime == None:\n",
    "            try:\n",
    "                cooktime = totaltime + preptime\n",
    "            except:\n",
    "                cooktime = None\n",
    "        dfout = dfout.append(pd.Series([cooktime, totaltime, preptime]), ignore_index = True)\n",
    "    dfout.columns = ['cooktime', 'totaltime', 'preptime']\n",
    "    return pd.concat([x, dfout], axis = 1).loc[:, ['name', 'stars', 'rev_count', 'pics', 'cals', 'fat_cal_pct', 'carb_cal_pct', 'protein_cal_pct', 'cholesterol_mg', 'sodium_mg', 'ing', 'cooktime', 'totaltime', 'preptime']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "\n",
    "def finalize(x):\n",
    "    return outputtimedf(ingred(transnut(main(x))))"
